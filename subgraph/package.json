{
  "name": "FluenceDeal",
  "license": "UNLICENSED",
  "scripts": {
    "compile": "graph codegen && graph build",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:studio:stage": "graph deploy --network mumbai --network-file config/networks.json fluence-deal-contracts",
    "create:local": "graph create --node http://localhost:8020/ fluence-deal-contracts",
    "remove:local": "graph remove --node http://localhost:8020/ fluence-deal-contracts",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 --network localhost --network-file config/networks.json --version-label 0.0.0 fluence-deal-contracts",
    "deploy:mumbai": "graph deploy --product hosted-service --network-file config/networks.json --network mumbai alcibiadescleinias/fluence-deal-contracts",
    "create:stage": "./fluence-graph.sh create",
    "deploy:stage": "./fluence-graph.sh deploy",
    "test": "graph test",
    "graph-node": "docker-compose up -d",
    "graph-local-clean": "docker-compose down -v"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.60.0",
    "@graphprotocol/graph-ts": "0.30.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
