{
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": "typechain-types",
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "declaration": true
    },
    "include": ["typechain-types/**/*"]
}
