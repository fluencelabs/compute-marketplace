{
  "extends": "@tsconfig/node16-strictest-esm/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "outDir": "dist",
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "remove",
    "noImplicitAny": false,
    "exactOptionalPropertyTypes": true
  }
}
