{
  "extends": "@tsconfig/node16-strictest-esm/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "target": "es2018",
    "module": "NodeNext",
    "declaration": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "remove",
    "noImplicitAny": true,
    "exactOptionalPropertyTypes": true
  }
}
